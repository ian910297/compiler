// A sample Rose program

procedure EMPTY is
    declare
        APPLE: integer; // declare a variable which name is APPLE
        CAR: integer; // declare a variable which name is CAR
        PIE: integer; // declare a variable which name is PIE
    begin
      APPLE := 50;

      for CAR in 0 .. APPLE loop
        PIE := PIE + CAR;
        write PIE;
      end loop;

      write PIE;
    end;
// end
