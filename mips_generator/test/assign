// A sample Rose program

procedure EMPTY is
    declare
        APPLE: integer; // declare a variable which name is APPLE
        BANANA: integer; // declare a variable which name is BANANA
        CAR: integer; // declare a variable which name is CAR
    begin
        // assign immediate value
        read APPLE;
        CAR := 100;
        write APPLE/CAR;
    end;
// end
